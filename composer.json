{
  "name": "bigcommerce/injector",
  "type": "library",
  "description": "Container interop Dependency Injector.",
  "keywords": [
    "di",
    "pimple",
    "injector"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Garry Filakhtov",
      "email": "garry.filakhtov@bigcommerce.com"
    },
    {
      "name": "Dmytro Kuzmenko",
      "email": "dmytro.kuzmenko@bigcommerce.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.2",
    "pimple/pimple": "*",
    "friendsofphp/proxy-manager-lts": "^1.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "phpspec/prophecy-phpunit": "^v2.3",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Bigcommerce\\Injector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.2"
    }
  }
}
